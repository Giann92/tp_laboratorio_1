#include "biblioteca.h"

float IngresarNumero(){
	float numero;

	scanf("%f", &numero);
while(numero <=0){
	printf("Error , numero no puede ser cero o negativo. reigrese por favor \n");
	scanf("%f", &numero);
}
	return numero;
}


float kmts(float x){
 float kmts;
	kmts = x;

	return kmts;
}

float PrecioVueloAerolineas(float y )
{
	float PrecioVueloAerolineas;

	PrecioVueloAerolineas = y;

	return PrecioVueloAerolineas;
}

float PrecioVueloLatam(float z){
 float PrecioVueloLatam;

	PrecioVueloLatam = z;

	return PrecioVueloLatam;
}

float DescTarjetaDevito(float z){

	float respuesta;

	respuesta =( z*10)/100;

	return respuesta;
}
float InteresTC(float num){

	float respuesta;


	respuesta = (num*25)/100;

	return respuesta;
}
long ValorBitcoin(long num){
	 long respuesta;
	 long bitcoin;
	 bitcoin = 4778869.90;

	 respuesta = num / bitcoin;

	 return respuesta;
}

float PrecioKmtrs(float num , float x){

	 float respuesta;


	 respuesta = num / x;

	 return respuesta;
}



